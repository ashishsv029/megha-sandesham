{
  "name": "megha-sandesham",
  "version": "1.0.0",
  "description": "A chatting application using websockets",
  "main": "dist/app.js",
  "scripts": {
    "start": "tsc && npm run copy-files && node dist/src/app.js",
    "start-debug": "tsc && npm run copy-files && node --inspect dist/src/app.js",
    "copy-files": "cp -r configs dist/",
    "dev": "nodemon --exec 'ts-node-esm' src/app.ts",
    "test": "nyc --reporter=lcov --reporter=text-summary mocha --exit",
    "test-debug": "nyc --reporter=lcov --reporter=text-summary mocha --exit --inspect",
    "lint": "eslint . --ext .ts"
  },
  "author": "ashish",
  "license": "ISC",
  "dependencies": {
    "-": "^0.0.1",
    "@prisma/client": "4.16.2",
    "@socket.io/redis-adapter": "^8.2.1",
    "@socket.io/redis-streams-adapter": "^0.1.0",
    "aws-sdk": "^2.1632.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "js-yaml": "^4.1.0",
    "redis": "^4.6.11",
    "save-dev": "^0.0.1-security",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.7.2",
    "tsconfig-paths": "^4.2.0",
    "typescript-ioc": "^3.2.2",
    "unique-names-generator": "^4.7.1"
  },
  "devDependencies": {
    "@types/chai": "^4.3.11",
    "@types/express": "^4.17.21",
    "@types/js-yaml": "^4.0.9",
    "@types/mocha": "^10.0.6",
    "@types/sinon": "^17.0.2",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "chai": "^4.3.10",
    "eslint": "^8.53.0",
    "mocha": "^10.2.0",
    "nodemon": "^3.0.1",
    "nyc": "^15.1.0",
    "prisma": "4.16.2",
    "sinon": "^17.0.1",
    "ts-node": "^10.9.1"
  }
}
